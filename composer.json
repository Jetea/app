{
    "name": "jetea/app",
    "description": "The Jetea Application.",
    "type": "project",
    "keywords": ["application", "jetea"],
    "license": "MIT",

    "require": {
        "php": ">=7.1.3",
        "jetea/framework": "~1.0",
        "phpctx/ctx": "~1.0",
        "jetea/config": "~2.0",
        "jetea/database": "~2.0",
        "predis/predis": "^1.1",
        "twig/twig": "^2.0",
        "vlucas/phpdotenv": "~2.2"
    },
    "suggest": {
        "phpctx/ctx": "Required to use ctx (~1.0)",
        "vlucas/phpdotenv": "Required to use .env files (~2.2).",
        "predis/predis": "Required to use predis (^1.1).",
        "twig/twig": "Required to use twig (~2.0)."
    },

    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Ctx\\": "ctx/"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
